{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "allowJs": true,
    "strict": true,
    "outDir": "./dist",
    "declaration": true,
    "declarationDir": "./dist/types",
    "emitDeclarationOnly": true,
    "noEmit": false,
    "allowImportingTsExtensions": true
  }
}
